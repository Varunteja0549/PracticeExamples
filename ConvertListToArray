public class ConvertListToArray {

	public static void main(String[] args) {
		List<String> list= new ArrayList<>();
		list.add("varun");
		list.add("teja");
		list.add("perla");
		System.out.println(list);
		String s[]= new String[list.size()];
		s=list.toArray(s);
		for(String str: s) {
			System.out.println(str);
		}
	}
}
